{
    "name": "aon4o/laravel-rocket-chat-notification-channel",
    "description": "Rocket.Chat REST API Notifications channel for Laravel 5.6+",
    "homepage": "https://github.com/aon4o/laravel-rocket-chat-notification-channel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "notifications",
        "notification-channel",
        "rocketchat",
        "rocket.chat",
        "chat"
    ],
    "authors": [
        {
            "name": "Alexander Naidenov",
            "email": "a.o.naidenov@gmail.com",
            "homepage": "https://github.com/aon4o",
            "role": "Developer"
        },
        {
            "name": "Anton Komarev",
            "email": "anton@komarev.com",
            "homepage": "https://komarev.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.0|^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "illuminate/notifications": "^5.6|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/queue": "^5.6|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^5.6|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.22",
        "mockery/mockery": "^1.3.3",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-drift": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\RocketChat\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\RocketChat\\RocketChatServiceProvider"
            ]
        }
    }
}
